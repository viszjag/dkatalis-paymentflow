<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Regresion Suite" thread-count="1" parallel="tests"
	verbose="1">
	<test name="Chrome Tests">
		<parameter name="browser" value="Chrome" />
		<classes>
			<class name="com.ui.tests.SuccessfuItemPurchase" />
			<class name="com.ui.tests.FailedItemPurchase" />
		</classes>
	</test>
	<test name="Firefox Tests">
		<parameter name="browser" value="Firefox" />
		<classes>
			<class name="com.ui.tests.SuccessfuItemPurchase" />
			<class name="com.ui.tests.FailedItemPurchase" />
		</classes>
	</test>
</suite>